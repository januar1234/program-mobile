void main() {
  // Mendefinisikan nilai
  double finalScore = 85.0;

  // Menggunakan SWITCH CASE untuk menentukan kelulusan
  String result;

  switch (finalScore ~/ 10) {
    case 10:
    case 9:
      result = 'Ferdi Reo LULUS dengan nilai A!';
      break;
    case 8:
      result = 'Ferdi Reo LULUS dengan nilai B!';
      break;
    case 7:
      result = 'Ferdi Reo LULUS dengan nilai C!';
      break;
    default:
      result = 'Ferdi Reo TIDAK LULUS.';
  }

  print(result);
}
