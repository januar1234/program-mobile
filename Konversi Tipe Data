void main() {
  // Konversi dari Integer ke String
  int januarAge = 21;
  String ageAsString = januarAge.toString();
  print('Umur januar dalam bentuk string: $ageAsString');

  // Konversi dari String ke Integer
  String januarScore = "100";
  int scoreAsInt = int.parse(januarScore);
  print('Skor januar dalam bentuk integer: $scoreAsInt');

  // Konversi dari Double ke Integer
  double januarHeight = 175.5;
  int heightAsInt = januarHeight.toInt();
  print('Tinggi januar dalam bentuk integer: $heightAsInt');

  // Konversi dari Boolean ke String
  bool januarIsStudent = true;
  String isStudentAsString = januarIsStudent.toString();
  print('januar seorang mahasiswa: $isStudentAsString');

  // Konversi dari String ke Double
  String januarWeight = "72.5";
  double weightAsDouble = double.parse(januarWeight);
  print('Berat badan januar dalam bentuk double: $weightAsDouble');
}
