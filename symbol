void main() {
  // Membuat simbol
  Symbol nameSymbol = Symbol('Ferdi Reo');
  Symbol ageSymbol = Symbol('Umur');

  // Menggunakan simbol dalam peta
  Map<Symbol, String> ferdiInfo = {
    nameSymbol: 'Ferdi Reo Tanuwidjaya',
    ageSymbol: '21 tahun',
  };

  // Menampilkan informasi menggunakan simbol
  print('${nameSymbol}: ${ferdiInfo[nameSymbol]}');
  print('${ageSymbol}: ${ferdiInfo[ageSymbol]}');
}
