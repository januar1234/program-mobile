void main() {
  // Daftar nama yang hanya berisi nama FERDI REO
  List<String> names = ['FERDI REO'];

  // Menggunakan fungsi anonim untuk memfilter nama yang mengandung "FERDI"
  var filteredNames = names.where((name) => name.contains('FERDI')).toList();

  print('Nama yang mengandung "FERDI":');
  filteredNames.forEach((name) {
    print(name); // Menampilkan nama yang mengandung "FERDI"
  });

  // Menggunakan fungsi anonim untuk mengubah nama menjadi huruf kecil
  var lowerCaseNames = names.map((name) => name.toLowerCase()).toList();

  print('Nama dalam huruf kecil:');
  lowerCaseNames.forEach((name) {
    print(name); // Menampilkan nama dalam huruf kecil
  });
}
