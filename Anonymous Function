void main() {
  // Daftar nama yang hanya berisi nama januar
  List<String> names = ['januar'];

  // Menggunakan fungsi anonim untuk memfilter nama yang mengandung "januar"
  var filteredNames = names.where((name) => name.contains('januar')).toList();

  print('Nama yang mengandung "januar":');
  filteredNames.forEach((name) {
    print(name); // Menampilkan nama yang mengandung "januar"
  });

  // Menggunakan fungsi anonim untuk mengubah nama menjadi huruf kecil
  var lowerCaseNames = names.map((name) => name.toLowerCase()).toList();

  print('Nama dalam huruf kecil:');
  lowerCaseNames.forEach((name) {
    print(name); // Menampilkan nama dalam huruf kecil
  });
}
