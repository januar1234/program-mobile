void main() {
  // Membuat list dengan nama "Ferdi" untuk menyimpan daftar buku
  List<String> ferdiBooks = ['Belajar Dart', 'Pemrograman Flutter', 'Pengembangan Web'];

  // Menampilkan isi list
  print('Daftar Buku Ferdi: ' + ferdiBooks.toString());

  // Menambahkan buku baru
  ferdiBooks.add('Dasar-Dasar Algoritma');
  print('Setelah menambahkan buku baru: ' + ferdiBooks.toString());

  // Menghapus buku
  ferdiBooks.remove('Pemrograman Flutter');
  print('Setelah menghapus buku: ' + ferdiBooks.toString());

  // Mengakses elemen list
  print('Buku pertama Ferdi: ' + ferdiBooks[0]);

  // Menampilkan jumlah buku
  print('Jumlah buku Ferdi: ' + ferdiBooks.length.toString());
}
