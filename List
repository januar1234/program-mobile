void main() {
  // Membuat list dengan nama "januar" untuk menyimpan daftar buku
  List<String> januarBooks = ['Belajar Dart', 'Pemrograman Flutter', 'Pengembangan Web'];

  // Menampilkan isi list
  print('Daftar Buku januar: ' + januarBooks.toString());

  // Menambahkan buku baru
  januarBooks.add('Dasar-Dasar Algoritma');
  print('Setelah menambahkan buku baru: ' + januarBooks.toString());

  // Menghapus buku
  januarBooks.remove('Pemrograman Flutter');
  print('Setelah menghapus buku: ' + januarBooks.toString());

  // Mengakses elemen list
  print('Buku pertama januar: ' + januar[0]);

  // Menampilkan jumlah buku
  print('Jumlah buku Ferdi: ' + januarBooks.length.toString());
}
