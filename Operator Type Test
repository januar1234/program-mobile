void main() {
  var januarName = "januar"; // Tipe data String
  var januarAge = 21;       // Tipe data int
  var januarHeight = 175.5; // Tipe data double

  // Menggunakan operator type test
  print('Apakah januarName adalah String? ${januarName is String}'); // true
  print('Apakah januarAge adalah int? ${januarAge is int}');         // true
  print('Apakah januarHeight adalah double? ${januarHeight is double}'); // true
  print('Apakah januarHeight adalah int? ${januarHeight is int}'); // false

  // Menggunakan operator type test untuk memeriksa tipe dan cast
  if (januarName is String) {
    print('Nama saya adalah ' + januarName + ' dengan panjang ' + januarName.length.toString() + ' karakter.');
  }
}
